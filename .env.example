# Client Configuration
# Phone number for the client (required)
# Format: +[country_code][number]
CLIENT_PHONE=+1234567890

# Working directory for client cache and session data
# Default: cache
CLIENT_WORK_DIR=cache

# Application version identifier
# Default: 25.12.13
CLIENT_APP_VERSION=25.12.13

# Device type identifier
# Default: DESKTOP
CLIENT_DEVICE_TYPE=DESKTOP


# Server Configuration
# Phone number for the server (required)
# Format: +[country_code][number]
SERVER_PHONE=+0987654321

# Working directory for server cache and session data
# Default: cache
SERVER_WORK_DIR=cache

# Application version identifier
# Default: 25.12.13
SERVER_APP_VERSION=25.12.13

# Device type identifier
# Default: DESKTOP
SERVER_DEVICE_TYPE=DESKTOP


# SOCKS5 Proxy Configuration
# Host address to bind the proxy server
# Default: 0.0.0.0 (all interfaces)
PROXY_HOST=0.0.0.0

# Port number for the proxy server
# Default: 1080
PROXY_PORT=1080

# Username for SOCKS5 authentication
# Default: username
PROXY_USERNAME=username

# Password for SOCKS5 authentication
# Default: password
PROXY_PASSWORD=password


# Logging Configuration
# Log level: DEBUG, INFO, WARNING, ERROR, CRITICAL
# Default: INFO
LOG_LEVEL=INFO
